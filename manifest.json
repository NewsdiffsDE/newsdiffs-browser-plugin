{
  "name": "Newsdiffs Deutschland",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Dieses Plugin befindet sich noch in der Entwicklung",
  "homepage_url": "http://www.newsdiffs.de",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "omnibox": {
    "keyword": "newsdiffs"
  },
  "permissions": [
    "tabs",
    "*://*.bild.de/*", 
    "*://*.zeit.de/*", 
    "*://*.focus.de/*", 
    "*://*.spiegel.de/*", 
    "*://*.stern.de/*", 
    "*://*.faz.de/*", 
    "*://*.rponline.de/*", 
    "*://*.sueddeutsche.de/*",
    "*://moreno-gummich.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.bild.de/*", 
        "*://*.zeit.de/*", 
        "*://*.focus.de/*", 
        "*://*.spiegel.de/*", 
        "*://*.stern.de/*", 
        "*://*.faz.de/*", 
        "*://*.rponline.de/*", 
        "*://*.sueddeutsche.de/*",
        "*://moreno-gummich.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ],
      "js": ["js/jquery/jquery.min.js", "js/lscache.js", "js/content.js"],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://*.bild.de/*", 
        "*://*.zeit.de/*", 
        "*://*.focus.de/*", 
        "*://*.spiegel.de/*", 
        "*://*.stern.de/*", 
        "*://*.faz.de/*", 
        "*://*.rponline.de/*", 
        "*://*.sueddeutsche.de/*",
        "*://moreno-gummich.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}